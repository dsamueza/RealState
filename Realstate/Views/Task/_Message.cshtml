
    <div class="post clearfix" style="background-color:white">
        <div class="box-header with-border">
            <h3 class="box-title">Nuevo Mensaje</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="form-group">
                <input class="form-control" id="des_msg" placeholder="Destinatario:">
            </div>
            <div class="form-group">
                <input class="form-control" id="subject_msg" placeholder="Asunto:">
            </div>
            <div class="form-group">
                <textarea  id="coment_msg" class="form-control" style="height: 150px" placeholder="Describe tu mensaje.."></textarea>
            </div>
            <div class="form-group">
                <form enctype="multipart/form-data"  id="formBacklink">
                    <div class="btn btn-default btn-file">
                     
                        <input id="file-es" name="file-es[]" type="file" multiple data-show-preview="false">
                    </div>

                </form>
                    <p class="help-block">Max. 5MB</p>
            </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            <div class="pull-right">

                <button type="submit" class="btn btn-primary" onclick="SaveCorreo()"><i class="fa fa-envelope-o"></i> Enviar</button>
            </div>

        </div>
    </div>
