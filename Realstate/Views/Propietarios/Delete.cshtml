@model Realstate.Models.BaseDatos.Propietario

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_RealState.cshtml";
}


    <div class="col-lg-offset-3 col-lg-6">
        <!-- left column -->
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="card-title">Elimimar Propietario</h3>
                    <h3>¿Esta seguro de eliminar el propietario?</h3>
                </div>
                <div class="box-body">
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Cedula)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Cedula)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Name)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Name)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Phone)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Phone)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Movil)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Movil)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Email)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Email)
                        </dd>
                    </dl>

                    <form asp-action="Delete">
                        <input type="hidden" asp-for="Id" />
                        <input type="submit" value="Delete" class="btn btn-primary" /> 
                        <a asp-action="Index" class="btn btn-primary">Salir</a>
                    </form>
                </div>
            </div>
        </div>
    </div>


