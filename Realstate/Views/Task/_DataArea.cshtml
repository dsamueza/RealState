
<div id="PredioTask">
    <!-- Profile Image -->
    <div v-for="(predio, index) in predios">
        <div class="box box-danger">
            <div class="box-body box-profile">

                <img class="profile-user-img img-responsive  img-circle" :src="predio.image" alt="User profile picture">



                <p class="text-muted text-center">Predio: {{predio.id}}</p>
                <div class="form-group">
                    <label>Estado</label>
                    <select name="statusRegister" class="form-control" v-model="predio.statusRegister">
                        <option value="A">Activo</option>
                        <option value="D">Desactivado</option>
                    </select>
                </div>
                <ul class="list-group list-group-unbordered">
                    <li class="list-group-item">
                        <b>Nombre</b> <a class="pull-right">{{predio.name}}</a>
                    </li>
                    <li class="list-group-item">
                        <b>Propietario</b>
                        <select v-model="predio.idPropietario" class="form-control" asp-items="@ViewBag.Propietario"></select>
                    </li>
                    <li class="list-group-item">
                        <b>Zona</b> <a class="pull-right">{{predio.zona}}</a>
                    </li>
                    <li class="list-group-item">
                        <b>Dimesiones</b> <a class="pull-right">{{predio.value}}</a>
                    </li>
                </ul>

                <a v-bind:href="'../UrbanProperty/AreasProspeccion?idProyecto=' + predio.idProyecto +'&IdArea='+predio.idZonaProspectada" class="btn bg-olive btn-flat margin pull-right"> Editar</a>
            </div>
            <!-- /.box-body -->
        </div>


        <div class="box box-danger">
            <div class="box-header with-border">
                <h3 class="box-title">Localización</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">

                <div id="map1" class="col-md-12" style="width:100%; height:400px;"></div>
                <div id="street1" class="col-md-12" style="width:100%; height:400px;"></div>
            </div>
            <!-- /.box-body -->
        </div>


        <div class="box box-default">
            <div class="box-header with-border">
                <h3 class="box-title">Multimedia</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <img alt="..." v-bind:src="predio.image" class="margin " style="width:100px; height:70px;" v-bind:id="predio.id" onclick="image(this)" >
            </div>

            <!-- /.box-body -->
        </div>
    </div>
    
</div>
<!-- /.box -->
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCZYA-xee_bKiCHQakUcDJtfVXagv_rhbA&callback=initialize">
</script>

